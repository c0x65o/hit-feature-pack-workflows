{
  "name": "@hit/feature-pack-workflow-core",
  "version": "1.0.44",
  "description": "A HIT feature pack",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./pages/*": {
      "development": "./src/pages/*.tsx",
      "types": "./dist/pages/*.d.ts",
      "import": "./dist/pages/*.js",
      "require": "./dist/pages/*.js",
      "default": "./dist/pages/*.js"
    },
    "./schema": {
      "development": "./src/schema/workflows.ts",
      "types": "./dist/schema/workflows.d.ts",
      "import": "./dist/schema/workflows.js",
      "require": "./dist/schema/workflows.js",
      "default": "./dist/schema/workflows.js"
    },
    "./server/api/*": {
      "development": "./src/server/api/*.ts",
      "types": "./dist/server/api/*.d.ts",
      "import": "./dist/server/api/*.js",
      "require": "./dist/server/api/*.js",
      "default": "./dist/server/api/*.js"
    }
  },
  "files": [
    "dist",
    "src",
    "feature-pack.yaml"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "peerDependencies": {
    "@hit/feature-pack-auth-core": "*",
    "@hit/sdk": "*",
    "@hit/ui-kit": "*",
    "drizzle-orm": "^0.38.0",
    "lucide-react": "^0.460.0",
    "next": "15.5.9",
    "react": "19.2.3"
  },
  "peerDependenciesMeta": {
    "@hit/sdk": {
      "optional": true
    },
    "@hit/ui-kit": {
      "optional": true
    }
  },
  "devDependencies": {
    "@hit/feature-pack-stubs": "file:../../../packages/hit-feature-pack-stubs",
    "@hit/feature-pack-auth-core": "file:../hit-feature-pack-auth-core",
    "@hit/sdk": "file:../../../hit-sdks/hit-sdk-typescript",
    "@hit/ui-kit": "file:../../../packages/hit-ui-kit-ts",
    "@types/node": "^20",
    "@types/react": "^19.0.0",
    "drizzle-orm": "^0.38.0",
    "lucide-react": "^0.460.0",
    "next": "^15.5.9",
    "typescript": "^5"
  },
  "keywords": [
    "hit",
    "feature-pack",
    "workflows"
  ],
  "license": "MIT",
  "sideEffects": false
}
